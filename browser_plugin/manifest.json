{
    "name": "Extension Name Placeholder",
    "version": "0.1",
    "manifest_version": 3,
    "description": "Extension description Placeholder",
    "permissions": [
        "storage",  
        "activeTab"
    ],
    "icons": {
        "16": "icons/icon-bitty.png Placeholder",
        "32": "icons/icon_32px_placeholder.png",
        "48": "icons/icon_48px_placeholder.png",
        "128": "icons/icon-large.png Placeholder"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["scripts/content_scripts.js"],
            "css": ["css/content.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["icons/icon_32px_placeholder.png"],
            "matches": ["<all_urls>"]
        }
    ],
    "background": {
        "background.scripts": "background.js"
    },
    "action": {
        "default_title": "Titulo do Plugin Placeholder",
        "default_icon": "icons/icon_48px_placeholder.png",
        "default_popup": "popup.html"
    }
}